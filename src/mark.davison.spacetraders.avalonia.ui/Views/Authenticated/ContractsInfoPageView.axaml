<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:vm="using:mark.davison.spacetraders.avalonia.ui.ViewModels.Authenticated"
             xmlns:v="using:mark.davison.spacetraders.avalonia.ui"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="mark.davison.spacetraders.avalonia.ui.ContractsInfoPageView"
	         x:DataType="vm:ContractsInfoPageViewModel">

	<Design.DataContext>
		<vm:ContractsInfoPageViewModel/>
	</Design.DataContext>

	<StackPanel>
		<TextBlock FontSize="64" Text="Contracts"/>

		<v:PagedDataGrid Meta="{Binding GridMeta}" />
	</StackPanel>
	
</UserControl>
