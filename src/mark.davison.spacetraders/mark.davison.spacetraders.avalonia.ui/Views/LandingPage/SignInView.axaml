<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:vm="using:mark.davison.spacetraders.avalonia.ui.ViewModels.LandingPage"
             mc:Ignorable="d"
             x:Class="mark.davison.spacetraders.avalonia.ui.SignInView"
             x:DataType="vm:RegisterViewModel">

	<Panel Margin="2">
		<Panel>
			<StackPanel HorizontalAlignment="Center" Width="200">
				<TextBlock Text="Sign In" />
				<Separator />
				<Label Content="Access Token" />
				<TextBox Text="" />
			</StackPanel>
		</Panel>
	</Panel>
</UserControl>
